<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.BEST.best" >


    <Button
        android:id="@+id/button_fb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="24dp"
        android:text="FB"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <Button
        android:id="@+id/button_insta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:text="Instagram"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/button_fb" />

    <Button
        android:id="@+id/button_lk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="LinkedIn"
        app:layout_constraintEnd_toEndOf="@+id/button_fb"
        app:layout_constraintStart_toStartOf="@+id/button_fb"
        app:layout_constraintTop_toBottomOf="@+id/button_fb" />

    <Button
        android:id="@+id/button_flr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="flickr"
        app:layout_constraintEnd_toEndOf="@+id/button_insta"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="@+id/button_insta"
        app:layout_constraintTop_toTopOf="@+id/button_lk" />

    <Button
        android:id="@+id/button_web"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Website"
        app:layout_constraintEnd_toStartOf="@+id/button_insta"
        app:layout_constraintStart_toEndOf="@+id/button_fb"
        app:layout_constraintTop_toTopOf="@+id/button_fb" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="296dp"
        android:layout_height="166dp"
        android:layout_marginTop="32dp"
        android:src="@drawable/ebec"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="180dp"
        android:layout_height="185dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/ipdj_logo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/partners_textView" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="180dp"
        android:layout_height="185dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:src="@drawable/uua_logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/partners_textView" />

    <TextView
        android:id="@+id/partners_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:text="Parceiros"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_lk" />

    <Button
        android:id="@+id/button_tt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Twitter"
        app:layout_constraintEnd_toEndOf="@+id/button_web"
        app:layout_constraintStart_toStartOf="@+id/button_web"
        app:layout_constraintTop_toTopOf="@+id/button_lk" />
</androidx.constraintlayout.widget.ConstraintLayout>